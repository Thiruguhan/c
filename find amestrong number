#include<stdio.h>
int main(){
    int n=1;
      while(n<10000000){
    int a[n];
    int sum=0;
    int c=0;
    int t1=n;
    int t2=n;
    while(n!=0){c++;
        n=n/10;
    }
    while(t1!=0){
    int l=t1%10;
    int p=1;
    int f=c;
    while(f!=0){
        p=p*l;  
        f--;
    }
        t1=t1/10;
         sum=sum+p;
    }if(sum==t2)
  {
    printf("%d \n",t2);    
  }
  n=t2;
  n++;
}
    return 0;
}
